#ifndef CUSTOM_SYSTICK_H_
#define CUSTOM_SYSTICK_H_


void systick_init(void);
void delayMs(uint16_t ms);
void task50ms(void);


void systick_init(uint16_t tickRate){
	volatile uint32_t reloadValue = (SystemCoreClock/tickRate) - 1;
}
